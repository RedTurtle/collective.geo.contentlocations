<html xmlns="http://www.w3.org/1999/xhtml"
      xmlns:metal="http://xml.zope.org/namespaces/metal"
      xmlns:tal="http://xml.zope.org/namespaces/tal"
      xmlns:i18n="http://xml.zope.org/namespaces/i18n"
      i18n:domain="collective.geo.contentlocations"
      tal:omit-tag="">


  <div tal:attributes="class view/css_class|nothing">

        <div style="float: right; width: 49%">
            <div tal:repeat="widget view/widgets/values" >
                <span tal:replace="widget/name" />
            </div>
        </div>
        <div style="float: left; width: 49%">
            <metal:use use-macro="context/@@ploneform-macros/titlelessform" />
        </div>
        <div class="visualClear" ><!-- clear floats --></div>

  </div>
</html>



